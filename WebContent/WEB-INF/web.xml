<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <listener>
    <listener-class>config.Initialize</listener-class>
  </listener>
  <listener>
    <listener-class>config.SynchronizeDataBase</listener-class>
  </listener>
  <listener>
    <listener-class>config.DefaultSettings</listener-class>
  </listener>
  <listener>
    <listener-class>config.SessionListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>signIn.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <location>/ErrorPage.jsp</location>
  </error-page>
</web-app>